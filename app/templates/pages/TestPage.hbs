{{! Display test code }}
<style>
.testcode {
	padding: 2px 4px;
	font-weight: bold;
	white-space: nowrap;
	background-color: #f7f7f9;
	border: 1px solid #d1d1d8;
}
.testcode_div {
    margin-bottom: 8px;
    margin-top: 4px;
}
</style>

<div class="testcode_div">
	<span class="text-muted"><b>Test </b></span>
	<span class="testcode">{{#if test.code}}{{test.code}}{{else}}unnamed{{/if}}</span>
	<span class="text-muted"><b> of </b></span><b>{{form.name}}</b>
</div>


<div id="contents"></div>

{{#if test.completed}}
<br/>
<div><button id="edit_button" type="button" class="btn btn-default margined"><span class="glyphicon glyphicon-edit"></span> Edit Test</button></div>
{{/if}}