{{#each tests}}
<tr class="tappable" id="{{_id}}">
  {{! TODO don't include name here directly }}
  <td><strong>{{name}}</strong></td>
  <td style="border-left:none;"><strong>{{#if source}}Source {{source}}{{/if}}</strong></td>
  <td style="border-left:none;">{{formatDate started "%F"}}</strong></td>
  <td style="vertical-align:middle; border-left:none; width: 16px"><i class="icon-chevron-right"></i></td>
</tr>
{{else}}
<tr>
    <td> <i>None</i> </td>
</tr>
{{/each}}